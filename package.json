{
  "name": "json-schema-pouchdb-app",
  "version": "0.1.0",
  "description": "A json schema database app built on pouchdb.",
  "keywords": [
    "pouchdb",
    "json",
    "json-schema",
    "db",
    "database",
    "react"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "test": "NODE_ENV=test jest --testRegex \"(src)/.*\\.test.jsx?$\"",
    "test:watch": "NODE_ENV=test jest --watch --testRegex \"(src)/.*\\.test.jsx?$\"",
    "lint": "prettier --check ./src/**"
  },
  "author": "Stan Lemon <stanlemon@users.github.com>",
  "license": "MIT",
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "verbose": true,
    "automock": false,
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^typeface-(.*)": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@loadable/component": "^5.14.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@rjsf/core": "^2.5.1",
    "@rjsf/material-ui": "^2.5.1",
    "ace-builds": "^1.4.12",
    "core-js": "^3.10.1",
    "date-fns": "^2.20.0",
    "notistack": "^1.0.5",
    "pouchdb": "^7.2.2",
    "react": "^17.0.1",
    "react-ace": "^9.4.0",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "slugify": "^1.5.0",
    "typeface-roboto": "1.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "lodash-webpack-plugin": "^0.11.6",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
