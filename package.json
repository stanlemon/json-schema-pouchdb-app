{
  "name": "json-schema-pouchdb-app",
  "version": "0.1.0",
  "description": "A json schema database app built on pouchdb.",
  "keywords": [
    "pouchdb",
    "json",
    "json-schema",
    "db",
    "database",
    "react"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server -w",
    "build": "NODE_ENV=production webpack",
    "test": "NODE_ENV=test jest --testRegex \"(src)/.*\\.test.jsx?$\"",
    "test:watch": "NODE_ENV=test jest --watch --testRegex \"(src)/.*\\.test.jsx?$\""
  },
  "author": "Stan Lemon <stanlemon@users.github.com>",
  "license": "MIT",
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "verbose": true,
    "automock": false,
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^typeface-(.*)": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@loadable/component": "^5.13.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@rjsf/core": "^2.4.0",
    "@rjsf/material-ui": "^2.4.0",
    "ace-builds": "^1.4.12",
    "date-fns": "^2.16.1",
    "notistack": "^1.0.1",
    "pouchdb": "^7.2.2",
    "react": "^16.14.0",
    "react-ace": "^9.1.4",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "slugify": "^1.4.5",
    "typeface-roboto": "0.0.75",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.5.3",
    "lodash-webpack-plugin": "^0.11.5",
    "pouchdb-adapter-memory": "^7.2.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  }
}
